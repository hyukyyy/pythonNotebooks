    "# quesion_1\n",
    "# daum 뉴스에서 2020.10.19일자 jtbc 뉴스 기사의 제목과 요약을 스크래핑해서 jtbc20201019.csv로\n",
    "# 저장하는 코드를 주피터 노트북으로 작성 (getJTBCnews)\n",
    "# 단, 1~3페이지만\n",
    "# https://media.daum.net/cp/310"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "from bs4 import BeautifulSoup\n",
    "import pandas as pd\n",
    "import requests"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 59,
   "metadata": {},
   "outputs": [],
   "source": [
    "def getJTBCnews(date='20201019',page=3):\n",
    "    \n",
    "    li_list=[]\n",
    "    title=[]\n",
    "    date=[]\n",
    "    press=[]\n",
    "    summary=[]\n",
    "    news_dict=dict()\n",
    "\n",
    "    for i in range(page):\n",
    "        url='https://media.daum.net/cp/310?page={}&regDate={}'.format(i,date)\n",
    "        html=requests.get(url).text\n",
    "        soup=BeautifulSoup(html,'html.parser')\n",
    "        li_list.extend(soup.select('#mArticle > div.box_etc > ul > li'))\n",
    "\n",
    "    for news in li_list:\n",
    "        title.append(news.select('div > strong')[0].text.strip().split('\\n')[0])\n",
    "        date.append(news.select('div > strong')[0].text.strip().split('\\n')[1].split()[1])\n",
    "        press.append(news.select('div > strong')[0].text.strip().split('\\n')[1].split()[0])\n",
    "        summary.append(news.select('div > div > span')[0].text.strip())\n",
    "\n",
    "    news_dict['title']=title\n",
    "    news_dict['summary']=summary\n",
    "    news_dict['date']=date\n",
    "    news_dict['press']=press\n",
    "\n",
    "    res=pd.DataFrame(news_dict)\n",
    "    \n",
    "    while True:\n",
    "        flag=input('저장하시겠습니까? (y/n)')\n",
    "        flag=flag.lower()\n",
    "        if flag=='y':\n",
    "            res.to_csv('{}.csv'.format(date))\n",
    "            return res\n",
    "        elif flag=='n':\n",
    "            return res\n",
    "        else:\n",
    "            print('잘못된 입력입니다.')\n",
    "            continue"
   ]
  },
